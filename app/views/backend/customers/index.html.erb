<div class="row employee-index-header">
  <div class="small-12 columns">
    <h3>Customers</h3>
  </div>
</div>
<%= link_to "New customer", new_backend_customer_path, class: "button radius" %>
<div class="row employee-list">
  <div class="small-12 columns">
    <ul>
      <% @customers.each do |customer| %>
        <li><%= link_to "#{customer.last_name} #{customer.first_name}", backend_customer_path(customer) %></li>
    <% end %>
  </ul>
  </div>
</div>
